<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        " http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="app.item.Item" table="item">
        <id name="id" type="int" column="ID">
            <generator class="identity" />
        </id>

        <!--<property name="name" type="string">-->
            <!--<column name="NAME" length="100" not-null="true" />-->
        <!--</property>-->

        <property name = "status" column = "status" type = "int"/>
        <property name = "receipt_id" column = "receipt_id" type = "int"/>
        <property name = "service_id" column = "service_id" type = "int"/>
        <property name = "cleaning_date" column = "cleaning_date" type = "java.util.Date"/>

    </class>
</hibernate-mapping>